(defmacro create_module (module_root module_name @rest body)
	`(let ((module qub))
		(if (listp ,m_name)
				(foreach nested_module ,m_name
					(setq module (get module (quote nested_module))))
				(setq module ,m_name))
		(putprop module 
		,@body))
